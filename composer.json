{
  "name": "zaplish/zaplish",
  "description": "TODO Zaplish CMS â€“ Laravel project skeleton.",
  "keywords": [
    "cms",
    "laravel",
    "content-management",
    "zap",
    "publish"
  ],
  "type": "project",
  "license": "proprietary",
  "repositories": [
    {
      "type": "path",
      "url": "../core",
      "options": {
        "symlink": true
      }
    }
  ],
  "require": {
    "php": "^8.2",
    "laravel/framework": "^12.0",
    "zaplish/core": "dev-main"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump"
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
